@model IEnumerable<Capstone.WebUI.Domain.Entities.PartnershipNight>

@{
    ViewBag.Title = "StoreProfile";
}

<h2>StoreProfile</h2>

<table class="table">
    <tr>
        <th>@Html.DisplayNameFor(model => model.StartDate)</th>
        <th>@Html.DisplayNameFor(model => model.EndDate)</th>
        <th>@Html.DisplayNameFor(model => model.CheckRequestId)</th>
        <th>@Html.DisplayNameFor(model => model.Comments)</th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>@Html.DisplayFor(modelItem => item.StartDate)</td>
        <td>@Html.DisplayFor(modelItem => item.EndDate)</td>
        <td>@Html.DisplayFor(modelItem => item.CheckRequestId)</td>
        <td>@Html.DisplayFor(modelItem => item.Comments)</td>
    </tr>
}

</table>
